<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.apps.android.viish.encyclolpedia"
      android:installLocation="auto"
      android:versionCode="1"
      android:versionName="0.1">
    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-sdk android:minSdkVersion="5" android:targetSdkVersion="15" />
    
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        
        <activity android:name=".Encyclolpedia"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait"
				  android:configChanges="orientation|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity android:name=".item.ShowItem" android:label="@string/app_name" android:screenOrientation="portrait" android:configChanges="orientation|keyboardHidden"> </activity>
		<activity android:name=".champion.ShowChampion" android:label="@string/app_name" android:screenOrientation="portrait" android:configChanges="orientation|keyboardHidden"> </activity>
		<activity android:name=".champion.Champions" android:label="@string/app_name" android:screenOrientation="portrait" android:configChanges="orientation|keyboardHidden"> </activity>
    </application>
</manifest> 